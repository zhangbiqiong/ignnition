{   
    "entities": [
        {
            "name": "link",
            "hidden_state_dimension": 32,
            "features":[
                {
                    "name": "link_capacity",
                    "normalization": "normalization_queue_size"
                }
            ]
        },
        {
            "name": "path",
            "hidden_state_dimension": 32,
            "features":
            [
                {
                    "name": "traffic",
                    "normalization": "normalization_queue_size"
                }
            ]
        },
        {
            "name": "node",
            "hidden_state_dimension": 32,
            "features":[
                {
                    "name": "queue_sizes",
                    "normalization": "normalization_queue_size"
                }
            ]
        }
    ],

    "mp_phase":
        {
            "num_iterations": 8,

            "architecture":
            [
                {
                    "step_name": "step1",
                    "mp_step":
                    [
                        {
                            "type": "multi_source",
                            "source_entity": "link",
                            "destination_entity":"path",
                            "adj_vector": "adj_links_paths",
                            "message": 
                            [
                                {
                                    "type": "None"
                                }
                            ],
                            "aggregation": "combination"
                        },
                        {
                            "type": "multi_source",
                            "source_entity": "node",
                            "destination_entity":"path",
                            "adj_vector": "adj_nodes_paths",
                            "message": 
                            [
                                {
                                    "type": "None"
                                }
                            ],
                            "aggregation": "combination"
                        }
                    ]
                },
                {
                    "step_name": "step2",
                    "mp_step":
                    [
                        {
                            "type": "single_source",
                            "source_entity": "path",
                            "destination_entity":"link",
                            "adj_vector": "adj_paths_links",
                            "message": 
                            [
                                {
                                    "type": "None"
                                }
                            ],
                            "aggregation": "sum",
                            "update": 
                            {
                                "type": "apply_rnn",
                                "recurrent_type": "GRU"
                            }
                            
                        },
                        {
                            "type": "single_source",
                            "source_entity": "path",
                            "destination_entity":"node",
                            "adj_vector": "adj_paths_nodes",
                            "message": 
                            [
                                {
                                    "type": "apply_nn",
                                    "nn_name": "aux",
                                    "input": ["hs_source"],
                                    "output_name": "hello"
                                },
                                {
                                    "type": "apply_nn",
                                    "nn_name": "aux",
                                    "input": ["hs_dest", "hello"]
                                }

                            ],
                            "aggregation": "sum",
                            "update": 
                            {
                                "type": "apply_rnn",
                                "recurrent_type": "GRU"
                            }
                            
                        }
                    ]
                }
            ],
            "combined_message_passing_options":
            [
                {
                    "step": "step1",
                    "destination_entity": "path",
                    "message_combination":"interleave",
                    "combination_definition": "path_interleave",
                    "update":
                    {
                        "recurrent_type": "GRU"
                    }
                }
            ]
        },


    "output": 
        [
            {
                "type_of_prediction": "node_prediction",
                "entity": "path",
                "output_label": "delay",
                "output_normalization": "normalization_queue_size",
                "nn_name": "readout_model"
            }
        ],
        
    "feed_forward_models":
    [
        {
            "model_name": "readout_model",
            "model_architecture":
            [
                {
                    "name": "First_dense_layer",
                    "type_layer":"Dense",
                    "units": 256,
                    "activation": "selu"
                },
                {
                    "name": "First_dropout_layer",
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "name": "Second_dense_layer",
                    "type_layer":"Dense",
                    "units": 256,
                    "activation": "selu"
                },
                {
                    "name": "Third_dropout_layer",
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "name": "Output_layer",
                    "type_layer":"Dense",
                    "units": 1,
                    "activation": "selu"
                }
            ]
        },
        {
            "model_name": "aux",
            "model_architecture":
            [
                {
                    "type_layer":"Dense",
                    "units": 256,
                    "activation": "selu"
                },
                {
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "type_layer":"Dense",
                    "units": 256,
                    "activation": "selu"
                },
                {
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "type_layer":"Dense",
                    "units": 32,
                    "activation": "selu"
                }
            ]
        }

    ],

    "learning_options":
    {
        "loss": "MeanSquaredError",
        "optimizer":
        {
            "type": "Adam"
        },
        "schedule": 
        {
            "type": "ExponentialDecay",
            "initial_learning_rate":0.001,
            "decay_steps":82000,
            "decay_rate":0.8,
            "staircase": "True"
        }
    }

}