{   
    "entities": 
    [
        {
            "name": "atom",
            "hidden_state_dimension": 16,
            "features":
            [
                {
                    "name": "element"
                },
                {
                    "name": "atomic_number"
                },
                {
                    "name": "acceptor"
                },
                {
                    "name": "donor"
                },
                {
                    "name": "aromatic"
                },
                {
                    "name": "hybridization"
                }
            ]
        }
    ],

    "mp_phase":
        {
            "num_iterations": 8,
            "architecture":
            [
                {
                    "step_name": "step1",
                    "message_passings":
                    [
                        {
                            "type": "single_source",
                            "source_entity": "atom",
                            "destination_entity":"atom",
                            "adj_vector": "adj_atom_atom",
                            "message": 
                            [
                                {
                                    "type": "apply_nn",
                                    "input":["hs_source, hs_dest"],
                                    "nn_name": "message_creation_nn"
                                }
                            ],
                            "agregation": "sum",
                            "update": 
                            {
                                "type": "apply_rnn",
                                "recurrent_type": "GRU"
                            }
                        }
                    ]
                }
            ]
        },

    "output": 
    [
        {
            "type": "node_prediction",
            "entity": "atom",
            "output_label": "mu",
            "output_normalization": "norm_mu",
            "output_denormalization": "denorm_mu",
            "nn_name": "readout"
        }
    ],

    "feed_forward_models":
    [
        {
            "nn_name": "readout",
            "nn_architecture":
            [
                {
                    "type_layer":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "type_layer":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "type_layer":"Dense",
                    "kernel_regularizer": 0.01,
                    "units": 1
                }
            ]
        },
        {
            "nn_name": "message_creation_nn",
            "nn_architecture":
            [
                {
                    "type_layer":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "type_layer":"Dense",
                    "kernel_regularizer": 0.01,
                    "units": 32
                }
            ]
        }
    ],
        
    "training_hyperparameters":
    {
        "loss": "MeanSquaredError",
        "optimizer":
        {
            "type": "Adam"
        },
        "schedule": 
        {
            "type": "PolynomialDecay",
            "initial_learning_rate":0.00001,
            "decay_steps":82000,
            "power":1
        }
    }

}