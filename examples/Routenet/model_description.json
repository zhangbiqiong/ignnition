{   
    "entities": 
    [
        {
            "name": "link",
            "hidden_state_dimension": 32,
            "features":
            [
                {
                    "name": "link_capacity"
                }
            ]
        },
        {
            "name": "path",
            "hidden_state_dimension": 32,
            "features":
            [
                {
                    "name": "traffic"
                }
            ]
        }
    ],

    "mp_phase":
        {
            "num_iterations": 8,

            "architecture":
            [
                {
                    "step_name": "step1",
                    "message_passings":
                    [
                        {
                            "type": "single_source",
                            "source_entity": "link",
                            "destination_entity":"path",
                            "adj_vector": "adj_links_paths",
                            "message": 
                            [
                                {
                                    "type": "None"
                                }
                            ],
                            "aggregation": "ordered",
                            "update": 
                            {
                                "type": "apply_rnn",
                                "recurrent_type": "GRU"
                            }
                            
                        }
                    ]
                },
                {
                    "step_name": "step2",
                    "message_passings":[
                        {
                            "type": "single_source",
                            "source_entity": "path",
                            "destination_entity":"link",
                            "adj_vector": "adj_paths_links",
                            "message": 
                            [
                                {
                                    "type": "None"
                                }
                            ],
                            "aggregation": "sum",
                            "update": 
                            {
                                "type": "apply_rnn",
                                "recurrent_type": "GRU"
                            }
                        }]
                }
            ]
        },


    "output": 
    [
        {
            "type_of_prediction": "node_prediction",
            "entity": "path",
            "output_label": "delay",
            "output_normalization": "log",
            "output_denormalization": "exp",
            "nn_name": "readout_model"
        }
    ],


    "feed_forward_models":
    [
        {
            "model_name": "readout_model",
            "model_architecture":
            [
                {
                    "name": "First_dense_layer",
                    "type_layer":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "name": "First_dropout_layer",
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "name": "Second_dense_layer",
                    "type_layer":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "name": "Third_dropout_layer",
                    "type_layer":"Dropout",
                    "rate":0.5
                },
                {
                    "name": "Output_layer",
                    "type_layer":"Dense",
                    "kernel_regularizer": 0.01,
                    "units": 1,
                    "activation": "None"
                }
            ]
        }

    ],
        
    "learning_options":
    {
        "loss": "MeanSquaredError",
        "optimizer":
        {
            "type": "Adam"
        },
        "schedule": 
        {
            "type": "ExponentialDecay",
            "initial_learning_rate":0.001,
            "decay_steps":82000,
            "decay_rate":0.8
        }
    }

}