{   
    "entities": 
    [
        {
            "name": "link",
            "hidden_state_dimension": 32,
            "features":
            [
                {
                    "name": "link_capacity"
                }
            ]
        },
        {
            "name": "path",
            "hidden_state_dimension": 32,
            "features":
            [
                {
                    "name": "traffic"
                }
            ]
        }
    ],

    "message_passing":
        {
            "num_iterations": 8,

            "architecture":
            [
                {
                    "step_name": "step1",
                    "messages":
                    [
                        {
                            "type": "individual",
                            "source_entity": "link",
                            "destination_entity":"path",
                            "message_formation": 
                            {
                                "message_neural_net": "no"
                            },
                            "agregation": "ordered",
                            "update": 
                            {
                                "update_type": "recurrent",
                                "recurrent_type": "GRU"
                            },
                            "adj_vector": "adj_links_paths"
                        }
                    ]
                },
                {
                    "step_name": "step2",
                    "messages":[
                        {
                            "type": "individual",
                            "source_entity": "path",
                            "destination_entity":"link",
                            "message_formation": 
                            {
                                "message_neural_net": "no"
                            },
                            "agregation": "sum",
                            "update": 
                            {
                                "update_type": "recurrent",
                                "recurrent_type": "GRU"
                            },
                            "adj_vector": "adj_paths_links"
                        }]
                }
            ]
        },


    "output": 
    [
        {
            "type": "local",
            "entity": "path",
            "output_label": "delay",
            "output_normalization": "log",
            "output_denormalization": "exp",
            "architecture":
            [
                {
                    "name": "First_dense_layer",
                    "type":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "name": "First_dropout_layer",
                    "type":"Dropout",
                    "rate":0.5
                },
                {
                    "name": "Second_dense_layer",
                    "type":"Dense",
                    "units": 256,
                    "kernel_regularizer": 0.1,
                    "activation": "selu"
                },
                {
                    "name": "Third_dropout_layer",
                    "type":"Dropout",
                    "rate":0.5
                },
                {
                    "type":"Dense",
                    "name": "Output_layer",
                    "kernel_regularizer": 0.01,
                    "units": 1,
                    "activation": "None"
                }
            ]
        }
    ],
        
    "training_hyperparameters":
    {
        "loss": "MeanSquaredError",
        "optimizer":
        {
            "type": "Adam"
        },
        "schedule": 
        {
            "type": "ExponentialDecay",
            "initial_learning_rate":0.001,
            "decay_steps":82000,
            "decay_rate":0.8
        }
    }

}